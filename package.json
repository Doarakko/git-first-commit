{
  "name": "git-first-commit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "format": "npx biome format --write",
    "lint": "npx biome lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "d1:execute": "wrangler d1 execute git-first-commit --local --file=./migrations/schema.sql",
    "d1:execute-prd": "wrangler d1 execute git-first-commit --remote --file=./migrations/schema.sql"
  },
  "dependencies": {
    "@next/third-parties": "15.1.7",
    "@octokit/core": "^6.1.4",
    "next": "15.1.7",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/next-on-pages": "^1.13.11",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@octokit/types": "^13.10.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^22",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "postcss": "^8",
    "tailwindcss": "^4.0.17",
    "typescript": "^5",
    "wrangler": "^4.6.0"
  },
  "overrides": {
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3"
  }
}