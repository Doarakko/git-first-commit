{
  "name": "git-first-commit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "format": "npx biome format --write",
    "lint": "npx biome lint",
    "pages:build": "npx @opennextjs/cloudflare build",
    "preview": "npx @opennextjs/cloudflare preview",
    "deploy": "npx @opennextjs/cloudflare build && npx @opennextjs/cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "d1:execute": "wrangler d1 execute git-first-commit --local --file=./migrations/schema.sql",
    "d1:execute-prd": "wrangler d1 execute git-first-commit --remote --file=./migrations/schema.sql"
  },
  "dependencies": {
    "@next/third-parties": "16.0.1",
    "@octokit/core": "^6.1.5",
    "next": "16.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@octokit/types": "^16.0.0",
    "@opennextjs/cloudflare": "^1.14.7",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^24",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "postcss": "^8",
    "tailwindcss": "^4.1.5",
    "typescript": "^5",
    "wrangler": "^4.45.3"
  },
  "overrides": {
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3"
  }
}
